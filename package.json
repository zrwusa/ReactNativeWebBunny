{
  "name": "react-native-web-bunny",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "web": "cross-env NODE_ENV=development webpack serve",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "web-build": "cross-env NODE_ENV=production webpack",
    "web-serve": "yarn web-build && http-server -P http://localhost:8080? ./public/",
    "mock": "node src/mock/server.js",
    "start": "yarn web",
    "start-native": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "watch": "jest -u --forceExit --detectOpenHandles --watchAll",
    "start-reset": "react-native start --reset-cache",
    "clear-jest": "jest --clearCache",
    "clear-tmp": "rm -rf $TMPDIR/metro-*; rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*",
    "clear-build": "rm -rf ./android/build; rm -rf ./android/.gradle; rm -rf ./android/app/build; rm -rf ./ios/build; cd ios; pod install --repo-update; cd ..; yarn jetify; cd android; ./gradlew clean; cd ..",
    "reset": "yarn clear-tmp; yarn clear-build; watchman watch-del-all; rm -rf ~/Library/Developer/Xcode/DerivedData"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/history": "^4.7.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.63.42",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-router-native": "^5.1.0",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "axios": "^0.21.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "fork-ts-checker-webpack-plugin": "^6.0.3",
    "history": "^4.10.1",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "json-server": "^0.16.3",
    "jsonwebtoken": "^8.5.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "^0.63.4",
    "react-native-web": "^0.14.9",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-native": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.0.0",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
